{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Web App",
            "type": "node-terminal",
            "request": "launch",
            "command": "npm run dev",
            "cwd": "${workspaceFolder}/Arahk.Neighbor.Web/web-app"
        },
        {
            "type": "coreclr",
            "name": "Debug Web Api",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Arahk.Neighbor.Api/bin/Debug/net8.0/Arahk.Neighbor.Api.dll",
            "cwd": "${workspaceFolder}/Arahk.Neighbor.Api"
        }
    ],
    "compounds": [
        {
            "name": "Debug All",
            "preLaunchTask": "build",
            "configurations": [
                "Debug Web App",
                "Debug Web Api"
            ]
        }
    ]
}